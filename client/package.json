{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && ng build",
    "clean": "rimraf dist",
    "format": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "format:all": "npx prettier --write .",
    "gen:api": "rimraf src/app/core/generated && openapi-generator-cli generate -c openapi.config.json -o src/app/core/generated",
    "gen:docs": "compodoc -p tsconfig.doc.json",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve --open --watch",
    "test": "ng test --code-coverage"
  },
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ngx-translate/core": "^17.0.0",
    "@tailwindcss/postcss": "^4.1.13",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.13",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.0",
    "@compodoc/compodoc": "^1.1.30",
    "@openapitools/openapi-generator-cli": "^2.23.4",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "^20.3.0",
    "eslint": "^9.36.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "onchange": "^7.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
